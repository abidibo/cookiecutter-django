{
    "_copy_without_render": [
        "*.scss",
        "*.css",
        "*.js",
        "*debug_toolbar/base.html",
        "*state/local.sls"
    ],
    "_extensions": ["cookiecutter.extensions.RandomStringExtension"],
    "project_name": "My New Project",
    "project_description": "My New Project description",
    "repo_name": "{{ cookiecutter.project_name|lower|replace(' ', '-') }}",
    "core_name": "core",
    "python_version": "3.9.5",
    "use_django_baton": "y",
    "use_filer": "n",
    "use_user_agents": "n",
    "use_simple_captcha": "n",
    "language_code": "en-us",
    "timezone": "Europe/Rome",
    "domain": "{{ cookiecutter.repo_name|replace('-', '') }}.it www.{{ cookiecutter.repo_name|replace('-', '') }}.it",
    "staging_domain": "{{cookiecutter.repo_name|replace('-', '')}}.sqrt64.it",
    "author": "",
    "email": "logs@otto.to.it",
    "local_user": "",
    "remote_user": "",
    "remote_password": "",
    "remote_root_mysql_pwd": "",
    "db_user": "{{ cookiecutter.remote_user }}",
    "db_user_pwd": "",
    "webapp_dir": "/home/{{ cookiecutter.remote_user }}/www/{{ cookiecutter.repo_name }}"
}
